		// 2**
		// 3***
		// 4****
		// 5*****
		// 이중 for 문을 이용해서 위의 별을 출력하세요
	for (int i = 1; i <= 5; i++) {
		System.out.println(i + "wpor ");
		for (int j = 1; j <= i ; j++) {
			
			System.out.println();
	
